{
  "name": "bellevue",
  "version": "0.5.0",
  "description": "A full-featured frontend project template for modern single-page applications built on Vue.js and Webpack.",
  "author": "Jerry JÃ¤ppinen <eiskis@gmail.com>",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Eiskis/bellevue.git"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "scripts": {
    "dev": "node tooling/dev-server.js",
    "start": "node tooling/dev-server.js",
    "build": "node tooling/build.js",
    "unit": "cross-env BABEL_ENV=test karma start tooling/unit/karma.conf.js",
    "unit:once": "cross-env BABEL_ENV=test karma start tooling/unit/karma.conf.js --single-run",
    "e2e": "node tooling/e2e/runner.js",
    "test": "npm run lint && npm run unit:once && npm run e2e",
    "docs": "npm run docs:components && npm run docs:directives && npm run docs:models && npm run docs:plugins && npm run docs:services && npm run docs:util && npm run docs:styles",
    "docs:components": "docco -o docs/components --languages docco.json src/components/**/*.vue",
    "docs:directives": "docco -o docs/directives src/vue/directives/*.js",
    "docs:mixins": "docco -o docs/mixins src/vue/mixins/*.js",
    "docs:models": "docco -o docs/models src/models/*.js",
    "docs:plugins": "docco -o docs/plugins src/vue/plugins/*.js",
    "docs:services": "docco -o docs/services src/services/*.js",
    "docs:util": "docco -o docs/util src/util/*.js",
    "docs:styles": "docco -o docs/styles src/styles/**/*.scss",
    "lint": "npm run lint:js && npm run lint:styles && npm run lint:vuestyles && npm run lint:html && npm run lint:vuehtml",
    "lint:js": "eslint --ext .js,.vue src spec",
    "help": "htmllint --help",
    "lint:html": "htmllint --cwd src/ --rc ./src/.htmllintrc **/*.html",
    "lint:vuehtml": "htmllint --cwd src/ --rc ./src/.htmllintrc **/*.vue",
    "lint:styles": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:vuestyles": "stylelint 'src/**/*.vue' --syntax scss",
    "lint:tooling": "eslint --ext .js tooling"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "es6-promise": "^4.1.1",
    "lodash": "^4.17.4",
    "natural-scroll": "^0.2.2",
    "vue": "^2.4.2",
    "vue-i18n": "^7.0.5",
